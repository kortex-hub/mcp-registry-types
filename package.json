{
  "name": "@kortex-hub/mcp-registry-types",
  "version": "1.4.0-next",
  "description": "TypeScript types for the Model Context Protocol (MCP) Registry API",
  "repository": "https://github.com/kortex-hub/mcp-registry-types",
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "license": "MIT",
  "type": "module",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.d.ts" 
    },
    "./getOpenAPISchema": {
      "default": "./src/getSchema.ts"
    }
  },
  "scripts": {
    "fetch": "ts-node src/fetchSchema.ts",
    "generate:schema": "npx openapi-typescript resources/openapi.yaml -o src/schema.d.ts --export-type",
    "generate": "pnpm run fetch && pnpm run generate:schema"
  },
  "dependencies": {
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^24",
    "openapi-typescript": "^7.8.0",
    "ts-node": "^10.9.2"
  },
  "packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a"
}
